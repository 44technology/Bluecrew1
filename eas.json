{
  "cli": {
    "version": ">= 16.23.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "image": "macos-sonoma-14.6-xcode-16.1"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "image": "macos-sonoma-14.6-xcode-16.1"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "image": "macos-sonoma-14.6-xcode-16.1",
        "env": {
          "NODE_OPTIONS": "--max-old-space-size=4096",
          "NODE_VERSION": "20"
        }
      },
      "node": "20"
    }
  },
  "submit": {
    "production": {}
  }
}